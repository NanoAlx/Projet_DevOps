pipeline {
    agent any
    parameters {
        booleanParam(name: 'FEVRIER_REDECORER', defaultValue: false, description: 'Avez-vous redécorer la maison pour vous sentir mieux?')
       
    }
    stages {
        stage('Confirmation des US de Février') {
            steps {
                echo "Confirmation des US pour Février"
                echo "redécorer la maison pour me sentir bien: ${params.JANVIER_JOURNAL}"
                
            }
        }
    }
}
