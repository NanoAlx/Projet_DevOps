pipeline {
    agent any
    parameters {
        choice(name: 'MOIS', choices: ['Janvier', 'Fevrier', 'Mars'], description: 'Choisissez le mois')
    }
    stages {
        stage('Appeler le mois') {
            steps {
                script {
                    def mois = "${params.MOIS}"

                    // Appel de la pipeline du mois correspondant
                    build job: "${mois}-Epic.groovy"
                }
            }
        }
    }
}
